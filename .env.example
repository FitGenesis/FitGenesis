# Application
NODE_ENV=development
PORT=4000

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/fitgenesis

# Solana
SOLANA_RPC_URL=https://api.devnet.solana.com
SOLANA_WALLET_PRIVATE_KEY=your_private_key_here

# JWT
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRES_IN=7d

# API Keys
GENE_ANALYSIS_API_KEY=your_gene_analysis_api_key_here
AI_SERVICE_API_KEY=your_ai_service_api_key_here

# External Services
GENE_ANALYSIS_API_URL=https://api.geneanalysis.example.com
AI_SERVICE_API_URL=https://api.aiservice.example.com

# Frontend
NEXT_PUBLIC_API_URL=http://localhost:4000
NEXT_PUBLIC_SOLANA_RPC_URL=https://api.devnet.solana.com

# Security
CORS_ORIGIN=http://localhost:3000
RATE_LIMIT_WINDOW=15m
RATE_LIMIT_MAX=100

# Monitoring
SENTRY_DSN=your_sentry_dsn_here
LOG_LEVEL=debug 